{
  "name": "e2e",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "e2e": "playwright test",
    "e2e:happo": "npx happo-e2e -- npx playwright test",
    "e2e:updateSnapshots": "npm run e2e -- --update-snapshots",
    "e2e:ignoreSnapshots": "CI=true npm run e2e -- --ignore-snapshots",
    "e2e:docker": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.28.0-focal"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "happo-e2e": "^2.1.0",
    "happo-playwright": "^2.0.1",
    "happo-plugin-typescript": "^1.0.0",
    "happo.io": "^8.3.1",
    "install": "^0.13.0",
    "npm": "^9.2.0"
  }
}
